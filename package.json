{
    "name": "js-node-electro-api",
    "version": "0.2.59",
    "private": true,
    "scripts": {
        "start": "pm2 start ecosystem.config.js --watch -i max --env production",
        "build": "export NODE_ENV=production && gulp clean && gulp default",
        "dev": "npm run devCluster & npm run devBuild",
        "devCluster": "nodemon dist/src/runCluster.js",
        "devBuild": "gulp dev",
        "images:resize": "gulp images:resize",
        "test:static": "prettier --check .",
        "test:unit": "gulp test:unit",
        "test": "npm run test:static & npm run test:unit ",
        "release": "standard-version"
    },
    "husky": {
        "hooks": {
            "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './_config/git/commitlint.js'"
        }
    },
    "nodemonConfig": {
        "ignore": [
            "assets/*",
            "templates/*"
        ]
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-customizable"
        },
        "cz-customizable": {
            "config": "_config/git/commitizen.js"
        }
    },
    "publishConfig": {
        "registry": "http://rpm.kiev.ua:4873"
    },
    "dependencies": {
        "@babel/register": "^7.12.1",
        "@sentry/node": "^5.26.0",
        "body-parser": "^1.19.0",
        "cropperjs": "^1.5.9",
        "express": "~4.17.1",
        "express-fileupload": "^1.2.0",
        "form-data": "^3.0.0",
        "google-auth-library": "^6.1.1",
        "node-fetch": "^2.6.1",
        "pm2": "^4.5.0",
        "pug": "^3.0.0",
        "sharp": "^0.26.2",
        "sqlite3": "^5.0.0",
        "uikit": "^3.5.8"
    },
    "devDependencies": {
        "@babel/core": "^7.12.1",
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/plugin-transform-runtime": "^7.12.1",
        "@babel/preset-env": "^7.12.1",
        "@commitlint/cli": "^11.0.0",
        "@fortawesome/fontawesome-free": "^5.15.1",
        "@jest/core": "^26.5.3",
        "@jest/globals": "^26.5.3",
        "babel-loader": "^8.1.0",
        "browser-sync": "^2.26.12",
        "css-loader": "^5.0.0",
        "cz-customizable": "^6.3.0",
        "gulp": "^4.0.2",
        "gulp-babel": "^8.0.0",
        "gulp-clean": "^0.4.0",
        "gulp-cli": "^2.3.0",
        "gulp-jest": "^4.0.3",
        "husky": "^4.3.0",
        "jest": "^26.5.3",
        "jest-cli": "^26.5.3",
        "less": "^3.12.2",
        "less-loader": "^7.0.2",
        "nodemon": "^2.0.5",
        "npm-check-updates": "^9.1.0",
        "plugin-error": "^1.0.1",
        "prettier": "2.1.2",
        "standard-version": "^9.0.0",
        "style-loader": "^2.0.0",
        "terser-webpack-plugin": "^4.2.3",
        "through2": "^4.0.2",
        "webpack": "^4.44.2",
        "webpack-stream": "^6.1.0"
    }
}
