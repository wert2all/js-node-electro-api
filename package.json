{
  "name": "js-node-electro-api",
  "version": "0.2.59",
  "private": true,
  "scripts": {
    "start": "pm2 start ecosystem.config.js --watch -i max --env production",
    "build": "export NODE_ENV=production && gulp clean && gulp default",
    "dev": "npm run devCluster & npm run devBuild",
    "devCluster": "nodemon dist/src/runCluster.js",
    "devBuild": "gulp dev",
    "test:static": "gulp test:static",
    "test:unit": "gulp test:unit",
    "test": "npm run test:static & npm run test:unit ",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test:static",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './_config/git/commitlint.js'"
    }
  },
  "nodemonConfig": {
    "ignore": [
      "assets/*",
      "templates/*"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "_config/git/commitizen.js"
    }
  },
  "publishConfig": {
    "registry": "http://rpm.kiev.ua:4873"
  },
  "dependencies": {
    "@sentry/node": "^5.22.0",
    "body-parser": "^1.19.0",
    "cropperjs": "^1.5.7",
    "express": "~4.17.1",
    "express-fileupload": "^1.2.0",
    "form-data": "^3.0.0",
    "google-auth-library": "^6.0.6",
    "node-fetch": "^2.6.0",
    "pm2": "^4.4.1",
    "pug": "^3.0.0",
    "sqlite3": "^5.0.0",
    "uikit": "^3.5.7"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@commitlint/cli": "^9.1.2",
    "@fortawesome/fontawesome-free": "^5.14.0",
    "@jest/core": "^26.4.2",
    "@jest/globals": "^26.4.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.12",
    "css-loader": "^4.2.2",
    "cz-customizable": "^6.3.0",
    "eslint": "^7.7.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-cli": "^2.3.0",
    "gulp-eslint": "^6.0.0",
    "gulp-jest": "^4.0.3",
    "husky": "^4.2.5",
    "jest": "^26.4.2",
    "less": "^3.12.2",
    "less-loader": "^7.0.0",
    "nodemon": "^2.0.4",
    "npm-check-updates": "^7.1.1",
    "plugin-error": "^1.0.1",
    "standard-version": "^9.0.0",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^4.1.0",
    "through2": "^4.0.2",
    "webpack": "^4.44.1",
    "webpack-stream": "^6.0.0"
  }
}
